{
    "name": "Lucidchart bars collapser",
    "manifest_version": 2,
    "version": "0.2",
    "description": "Remove menubar, tabbar, footerbar & \"try paid\" message from document page",
    "permissions": ["https://www.lucidchart.com/*"],
    "content_scripts": [{
        "matches": ["https://www.lucidchart.com/documents/*"],
        "js": ["jquery-1.7.2.min.js", "plugin.js"],
        "all_frames": true
    }]
}

